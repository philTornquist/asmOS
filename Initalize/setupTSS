setupTSS:
event "Setting up TSS"

mov ax, 0x2B
ltr ax

mov [TSS+0x04], dword SYSSTACK
mov [TSS+0x08], dword 0x10
mov [TSS+0x1C], dword PAGE_DIRECTORY
mov [TSS+0x48], dword 0x13
mov [TSS+0x4C], dword 0x0B
mov [TSS+0x50], dword 0x13
mov [TSS+0x54], dword 0x13
mov [TSS+0x58], dword 0x13
mov [TSS+0x5C], dword 0x13